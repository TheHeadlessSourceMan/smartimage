<?xml version="1.0" encoding="utf-8"?>
<smartimage>
	
	<!-- solid color -->
	<solid color="#00AA00" w="200" h="200" x="0" y="0" />
	
	<!-- clouds -->
	<texture type="clouds" w="200" h="200" x="200" y="0" />
	
	<!-- marble -->
	<texture noise="0.15" direction="45" frequency="15,15" w="200" h="200" x="400" y="0" />
	
	<!-- wood -->
	<texture noise="0.15" direction="circular" frequency="12" w="200" h="200" x="600" y="0" />
	
	<!-- SECOND ROW -->
	
	<!-- sphere -->
	<texture noise="0" direction="circular" frequency="0.5" w="200" h="200" x="0" y="200" />
	
	<!-- cell noise -->
	<texture type="voronoi" w="200" h="200" x="200" y="200" />
	
	<!-- blobs -->
	<texture type="voronoi" invert="true" w="200" h="200" x="400" y="200" />
	
	<!-- light beam -->
	<texture type="clock" noise="0" angle="45" waveform="sine" w="200" h="200" x="600" y="200" />
	
</smartimage>